{
  "name": "sphinx_mobigen_theme",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all build:docs",
    "build:parcel": "parcel build --no-source-maps src/js/sphinx_mobigen_theme.js -d sphinx_mobigen_theme/static",
    "build:docs": "rimraf _build && sphinx-build -b html . ../manual"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "parcel-bundler": "^1.10.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "material-design-lite": "^1.3.0"
  }
}
